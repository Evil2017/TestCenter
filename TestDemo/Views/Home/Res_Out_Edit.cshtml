﻿@{
    ViewBag.Title = "Res_Out_Edit";
    string restype = "2";
    string resname = "物料";
    switch (restype)
    {
        case "1":
            resname = "耗材";
            break;
        case "2":
            resname = "试剂";
            break;
        case "3":
            resname = "辅材";
            break;
        case "4":
            resname = "疫苗";
            break;
        case "5":
            resname = "办公用品"; break;
        case "6":
            resname = "物品";
            break;
        default:
            break;
    }
}
<style type="text/css">
    .red{
        color: #f00;
    }
    .dm-td3 {
        width:100px;
    }
</style>
 <div class="dm-zl-box">
     <form id="Test"  method="post" >
         <table border="0" width="600" cellpadding="0" cellspacing="0" style="margin-top:20px;">
             <tr>
                 <td class="dm-td3">物料名称：</td>
                 <td>
                     <input type="text"  class="dm-input1"   id="WLName" name="WLName" />
                     <input type="hidden" name="ID" id="ID" value="" />
                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">生产厂家：</td>
                 <td>
                     <input type="text" class="dm-input1" name="Fac" id="Fac"/>
                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">规格型号：</td>
                 <td>
                     <input type="text" class="dm-input1" name="GG" id="GG"/>

                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">生产批号：</td>
                 <td>
                     <input type="text" class="dm-input1" name="BetNO" id="BetNO"  />

                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">仓库库存：</td>
                 <td>
                     &nbsp;&nbsp;&nbsp;<label id="NUM"  name="NUM">0</label>
                     <input type="checkbox" onclick="ha()" id="a" name="a" value="1" />
                     @*修正库存
                     <input type="number" class="dm-input2" name="SYNUM" id="SYNUM" style="display:none"   />
                     <input type="hidden"   name="XzNum" id="XzNum" />
                     <input type="hidden"  name="XzNum2" id="XzNum2" />*@
                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">单位：</td>
                 <td>
                     <input type="text" class="dm-input1" name="DW" id="DW"  />
                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">出库数量：</td>
                 <td>
                     <input type="text" class="dm-input1" name="CNum" id="CNum" onkeydown="datetimeboxSelect('Cdate')"  />
                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">出库日期：</td>
                 <td>
                     <input type="text"  class="easyui-datebox"  name="Cdate" id="Cdate" onkeydown="easySelect('Name')" value="@DateTime.Now.ToString("yyyy-MM-dd")"  />
                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">操作者：</td>
                 <td>
                     <input type="text" class="dm-input1" id="Name" name="Name" onfocus="UserSign(this.id,'操作员签名','Storage','0')"  />

                     <input type="hidden"   ID="FID" name="FID" />
                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">复核者：</td>
                 <td>
                     <input type="text" class="dm-input1" id="SHName" name="SHName"  onfocus="UserSign(this.id,'操作员签名','Storage','1')" />

                     <input type="hidden"   ID="SHFID" name="SHFID" />
                 </td>
             </tr>
             <tr>
                 <td class="dm-td3">备注：</td>
                 <td>
                     <input type="text" class="dm-input1" name="Remark" id="Remark"  onkeydown="CheckOnClick('ok')"  />

                 </td>
             </tr>
             <tr>
                 <td class="dm-td3"></td>
                 <td>
                     <button class="dm-btn-ck" id="ok" onclick="save()" type="button">保存</button>
                 </td>
             </tr>
         </table>
     </form>
 </div>