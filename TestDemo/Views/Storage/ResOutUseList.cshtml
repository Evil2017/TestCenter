 @{
    ViewBag.Title = "ResOutUseList";
       string restype = "2"; string Place = "1";
   // string BictNo = ViewBag["BictNo"];
    string resname = "物料";
    switch (restype)
    {
        case "1":
            resname = "耗材";
            break;
        case "2":
            resname = "试剂";
            break;
        case "3":
            resname = "辅材";
            break;
        case "4":
            resname = "疫苗";
            break;
        case "5":
            resname = "办公用品";
            break;

        default:
            break;
    }
}
 <style type="text/css">
    .panel-body{
        background-color: #fff;
        border:none;
    }
</style>
<div data-options="region:'center'">
    <table id="jyjl" fit="true"></table>
    <div style="background-color:#fff; padding:5px 0px 5px 5px;" id="seachtoolwx">
        &nbsp;&nbsp;
        <label>起始时间：</label>
        <input type="text" name="Stime" id="Stime" class="easyui-datebox">
        <label>到</label>
        <input type="text" name="Etime" id="Etime" class="easyui-datebox">
         &nbsp;&nbsp;
        <label>材料名称</label>
          <input type="text" class="dm-input1" ID="ResName" onkeydown="OK()"/>
          &nbsp;&nbsp;
        <label>生产厂家</label>
          <input type="text" class="dm-input1" ID="Factory"onkeydown="OK()" />
        &nbsp;&nbsp;
         <label>包含0库存</label>
          <input type="checkbox"   ID="ifkc" />

        <button class="dm-btn-ck" onclick="Search()" type="button">搜索</button>

    </div>
</div>
<script>
    $(function () { init();});

    function init() {
        $('#jyjl').datagrid({
            url: "",
            queryParams: {},
            toolbar: '#seachtoolwx',
            onDblClickRow: function (index, row) { },
            fit: true,
            columns: [[

                    { field: 'oDate', title: '出库日期', width: 130 },
                    { field: 'ResName', title: '物料名称', width: 180 },
                    { field: 'Factory', title: '厂家', width: 180 },
                    { field: 'Spec', title: '规格', width: 90 },

                    { field: 'bictNO', title: '批号', width: 90 },
                     { field: 'OutDate', title: '有效期', width: 90 },
                      { field: 'OutNum', title: '出库数量', width: 90, align: "center" },
                       { field: 'BFNum', title: '报废数量', width: 90, align: "center" },
                       { field: 'BFUser', title: '报废人', width: 80 },
                       { field: 'Remark', title: '备注', width: 120 },
                       {
                           field: 'opts', title: '操作', width: 180, formatter: function (i, r) {

                               return "<a href='#'  onclick='Edit(" + r.ID + ")'>登记报废</a> ";
                           }
                       },
                    //{ field: 'opts', title: '操作', width: 50 },

            ]]
        });
    }

</script>