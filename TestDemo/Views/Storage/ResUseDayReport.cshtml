
@{
    ViewBag.Title = "ResUseDayReport";
       string restype = "2";
   // string BictNo = ViewBag["BictNo"];
    string resname = "物料";
    switch (restype)
    {
        case "1":
            resname = "耗材";
            break;
        case "2":
            resname = "试剂";
            break;
        case "3":
            resname = "辅材";
            break;
        case "4":
            resname = "疫苗";
            break;
        case "5":
            resname = "办公用品";
            break;

        default:
            break;
    }
}
 <style type="text/css">
    .panel-body{
        background-color: #fff;
        border: none;
    }
</style>
<div data-options="region:'center'">
    <table id="jyjl" fit="true"></table>
    <div style="background-color:#fff; padding:5px 0px 5px 5px;" id="seachtoolwx">
        &nbsp;&nbsp;
        <label>出库时间：</label>
        <input type="text" name="Stime" id="Stime" class="easyui-datebox">
        <label>到</label>
        <input type="text" name="Etime" id="Etime" class="easyui-datebox">
         &nbsp;&nbsp;
        <label>批号</label>
          <input type="text" class="dm-input1" ID="ResName" onkeydown="OK()"/>
          &nbsp;&nbsp;
        <button class="dm-btn-ck" onclick="Search()" type="button">搜索</button>
        <button class="dm-btn-ck" onclick="out()" type="button">导出</button>

    </div>
</div>
<script>
    $(function () { init();});
      function init() {
        $('#jyjl').datagrid({
            url: "",
            queryParams: {},
            toolbar: '#seachtoolwx',
            onDblClickRow: function (index, row) { },
            fit: true,
            columns: [[
                    { field: 'UseDate', title: '日期', width: 70 },
                    { field: 'ResName', title: '材料名称', width: 90 },
                    { field: 'Factory', title: '生产厂家', width: 90 },
                    { field: 'pz', title: '批准文号', width: 90 },
                    { field: 'spec', title: '规格', width: 90 },
                    { field: 'BictNo', title: '批号', width: 90 },
                    { field: 'OldStockNum', title: '上日库存总数', width: 90 },
                    { field: 'OutNum', title: '出库数量', width: 90 },
                    {
                        field: 'UseNum', title: '有效数量', width: 90, formatter: function (i, r) {
                            return "<span style='color:green;'>" + r.UseNum + "</span>";
                    }
                    },
                    {
                        field: 'WXNum', title: '无效数量', width: 90, formatter: function (i, r) {
                            return "<span style='color:#6395DE;'>" + r.WXNum + "</span>";
                    }
                    },
                    {
                        field: 'BFNum', title: '报损数量', width: 90, formatter: function (i, r) {
                            return "<span style='color:red;'>" + r.BFNum + "</span>";
                    }
                    },
                     {
                         field: 'NaclWxNum', title: '未使用数量', width: 90, formatter: function (i, r) {
                             return "<span style='color:red;'>" + r.BFNum + "</span>";
                         }
                     },
                    {
                        field: 'AllNum', title: '实用数量', width: 90, formatter: function (i, r) {
                            return "<span style='color:#b200ff;'>" + r.AllNum + "</span>";
                    }
                    },

                    { field: 'lastNum', title: '当前总库存数量', width: 90 },
                    //{ field: 'opts', title: '操作', width: 50 },

            ]]
        });
    }

</script>