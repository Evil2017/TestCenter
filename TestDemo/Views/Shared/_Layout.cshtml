        function easySelect(NextInput) {
            if (event.keyCode == 13 || (event.ctrlKey && event.keyCode == 77) || event.keyCode == 40) {
                $("#" + NextInput + "").focus().select();
            }
        }

        function CheckOnClick(NextInput) {//回车点击事件

            if (event.keyCode == 13 || (event.ctrlKey && event.keyCode == 77) || event.keyCode == 40) {
                document.getElementById(NextInput).click();
                event.returnValue = false;
            }
        }

        function UserSign(CheckTxtBox, title) {
            $("#SignCheck").dialog({
                width: 240,
                height: 200,
                modal: true,
                iconCls: 'icon-tip',
                title: title,
                buttons: [{
                    text: '确定',
                    iconCls: 'icon-ok',
                    handler: function () {
                        UserSignCheck();
                    }
                }, {
                    text: '取消',
                    iconCls: 'icon-cancel',
                    handler: function () {
                        $('#SignCheck').dialog('close');
                    }
                }]
            });
            $("#SignCheck").dialog("open");
            $("#SignCheck").dialog("center");
            // $("#SignCheckLoginName").focus();
            //setTimeout("formInt()", 5000);
            document.getElementById("SignCheckLoginName").focus();
            document.getElementById("SignCheckLoginName").value = "";
            document.getElementById("SignCheckPwd").value = "";
            document.getElementById("SignBox").value = CheckTxtBox;
            document.getElementById("SignResultLab").innerHTML = "";

        }

        function UserSignCheck() {
            var refs = "0";
            CheckTxtBox = document.getElementById("SignBox").value;
            $.post("/Home/CheckUserLogin", { loginName: document.getElementById("SignCheckLoginName").value, pwd: document.getElementById("SignCheckPwd").value }, function (data, status) {
                if (data.indexOf('|') > -1) {
                    document.getElementById("SignResultLab").innerHTML = data.split('|')[1];
                }else{
                if (data != "0") {
                    document.getElementById(CheckTxtBox).value = data;
                    if (CheckTxtBox == "SendOperator") {
                        top.$('#checkOne').val(data);
                    }
                    if (CheckTxtBox == "ChekOperator") {
                        top.$('#checkTwo').val(data);
                    }
                    $('#SignCheck').dialog('close');
                }
                }

            });
            return refs;
        }
        function LayerUserSignCheck() {
            var refs = "0";

            $.post("/Home/CheckUserLogin", { loginName: document.getElementById("layerLoginName").value, pwd: document.getElementById("layerCheckPwd").value }, function (data, status) {
                if (data.indexOf('|') > -1) {
                    document.getElementById("layerResultLab").innerHTML = data.split('|')[1];
                } else {
                    if (data != "0") {
                        SignCheckBack();
                        $("#layerCheckPwd").val('');
                    }
                }

            });
            return refs;
        }

        function printCtrsss(printDate) {
