    })

    function ChangePass() {
        var thisval = "";
        var SampleType = $("#SampleType").val();
        
        if ("InputType" == "1") {
            thisval = $(".TypeInput").val();
            if (SampleType == "11") {
                if (thisval >= "50") {
                    ifPass = true;
                }
                else {
                    ifPass = false;
                }
            } else {
                if (thisval >= "60") {
                    ifPass = true;
                }
                else {
                    ifPass = false;
                }
            }

        }
        else {
            thisval = $(".TypeSelect").val();
            if (SampleType == "11") {
                if (thisval == "≥50") {
                    ifPass = true;
                }
                else {
                    ifPass = false;
                }
            } else {
                if (thisval == "≥60") {
                    ifPass = true;
                }
                else {
                    ifPass = false;
                }
            }
        }

        var v = ifPass ? "1" : "2";
        $("#Result").val(v);
        //$('#Result').combobox('setValue', v);
    }

    function ChangeTypeSelect() {
        var htm = "";
        if ("InputType"=="0") {
            var SampleType = $("#SampleType").val();
            if (SampleType == "11") {
                htm = '<select id="SmallValue"  class="TypeSelect"  field="SmallValue"> <option value="≥50">≥50</option> <option value="＜50">＜50</option></select>';
            } else {
                htm = '<select id="SmallValue"  class="TypeSelect"  field="SmallValue"> <option value="≥60">≥60</option> <option value="＜60">＜60</option></select>';
            }
            $("#optionHtml").html(htm);
        }
    }

